{
    "Align(ed)": {
        "prefix": "ali",
        "body": [
            "\\begin{align`echo $TM_SCOPE|grep math|",
            "sed -e 's/.*math.*/ed/'`}",
            "\t$0",
            "\\end{align`echo $TM_SCOPE|grep math|",
            "sed -e 's/.*math.*/ed/'`}"
        ],
        "desciption": "Align(ed)"
    },
    "Cases": {
        "prefix": "cas",
        "body": [
            "\\begin{cases}",
            "\t${1:equation}, &\\text{ if }${2:case}\\\\\\\\",
            "\t$0",
            "\\end{cases}"
        ],
        "desciption": "Cases"
    },
    "Chapter": {
        "prefix": "cha",
        "body": [
            "\\chapter{${1:chapter name}} % (fold)",
            "\\label{cha:${2:${1/(\\w+)(\\W+$)?|\\W+/${1:?${1:/asciify/downcase}:_}/g}}}",
            "${0:$TM_SELECTED_TEXT}",
            "% chapter $2 (end)"
        ],
        "desciption": "Chapter"
    },
    "Description": {
        "prefix": "desc",
        "body": [
            "\\\\begin{description}",
            "\t\\item[$1] $0",
            "\\\\end{description}"
        ],
        "desciption": "Description"
    },
    "Display Math — \\[ … \\]": {
        "prefix": "$$",
        "body": [
            "\\[",
            "\t$TM_SELECTED_TEXT$1",
            "\\]"
        ],
        "desciption": "Display Math"
    },
    "Enumerate": {
        "prefix": "enum",
        "body": [
            "\\\\begin{enumerate}",
            "\t\\item $0",
            "\\\\end{enumerate}"
        ],
        "desciption": "Enumerate"
    },
    "Equation": {
        "prefix": "eq",
        "body": [
            "\\begin{equation}",
            "\t$0",
            "\\end{equation}"
        ],
        "desciption": "Equation"
    },
    "Figure": {
        "prefix": "figure",
        "body": [
            "${1:Figure}~\\ref{${2:fig:}}$0"
        ],
        "desciption": "Figure"
    },
    "Gather(ed)": {
        "prefix": "gat",
        "body": [
            "\\begin{gather`echo $TM_SCOPE|grep math|",
            "sed -e 's/.*math.*/ed/'`}",
            "\t$0",
            "\\end{gather`echo $TM_SCOPE|grep math|",
            "sed -e 's/.*math.*/ed/'`}"
        ],
        "desciption": "Gather(ed)"
    },
    "Itemize": {
        "prefix": "item",
        "body": [
            "\\\\begin{itemize}",
            "\t\\item $0",
            "\\\\end{itemize}"
        ],
        "desciption": "Itemize"
    },
    "Listing": {
        "prefix": "listing",
        "body": [
            "${1:Listing}~\\ref{${2:lst:}}$0"
        ],
        "desciption": "Listing"
    },
    "Matrix": {
        "prefix": "mat",
        "body": [
            "\\begin{${1:p/b/v/V/B/small}matrix}",
            "\t$0",
            "\\end{${1:p/b/v/V/B/small}matrix}"
        ],
        "desciption": "Matrix"
    },
    "Page": {
        "prefix": "page",
        "body": [
            "${1:page}~\\pageref{$2}$0"
        ],
        "desciption": "Page"
    },
    "Paragraph": {
        "prefix": "par",
        "body": [
            "\\paragraph{${1:paragraph name}} % (fold)",
            "\\label{par:${2:${1/(\\w+)(\\W+$)?|\\W+/${1:?${1:/asciify/downcase}:_}/g}}}",
            "${0:$TM_SELECTED_TEXT}",
            "% paragraph $2 (end)"
        ],
        "desciption": "Paragraph"
    },
    "Part": {
        "prefix": "part",
        "body": [
            "\\part{${1:part name}} % (fold)",
            "\\label{prt:${2:${1/(\\w+)(\\W+$)?|\\W+/${1:?${1:/asciify/downcase}:_}/g}}}",
            "${0:$TM_SELECTED_TEXT}",
            "% part $2 (end)"
        ],
        "desciption": "Part"
    },
    "Region Start": {
        "prefix": "#region",
        "body": [
            "%#Region $0"
        ],
        "description": "Folding Region Start"
    },
    "Region End": {
        "prefix": "#endregion",
        "body": [
            "%#Endregion"
        ],
        "description": "Folding Region End"
    },
    "Section": {
        "prefix": "section",
        "body": [
            "${1:Section}~\\ref{${2:sec:}}$0"
        ],
        "desciption": ""
    },
    "Split": {
        "prefix": "spl",
        "body": [
            "\\begin{split}",
            "\t$0",
            "\\end{split}"
        ],
        "desciption": "Section"
    },
    "Sub Paragraph": {
        "prefix": "subp",
        "body": [
            "\\subparagraph{${1:subparagraph name}} % (fold)",
            "\\label{subp:${2:${1/(\\w+)(\\W+$)?|\\W+/${1:?${1:/asciify/downcase}:_}/g}}}",
            "${0:$TM_SELECTED_TEXT}",
            "% subparagraph $2 (end)"
        ],
        "desciption": "Sub Paragraph"
    },
    "Sub Section": {
        "prefix": "sub",
        "body": [
            "\\subsection{${1:subsection name}} % (fold)",
            "\\label{sub:${2:${1/(\\w+)(\\W+$)?|\\W+/${1:?${1:/asciify/downcase}:_}/g}}}",
            "${0:$TM_SELECTED_TEXT}",
            "% subsection $2 (end)"
        ],
        "desciption": "Sub Section"
    },
    "Sub Sub Section": {
        "prefix": "subs",
        "body": [
            "\\subsubsection{${1:subsubsection name}} % (fold)",
            "\\label{ssub:${2:${1/(\\w+)(\\W+$)?|\\W+/${1:?${1:/asciify/downcase}:_}/g}}}",
            "${0:$TM_SELECTED_TEXT}",
            "% subsubsection $2 (end)"
        ],
        "desciption": "Sub Sub Section"
    },
    "Table": {
        "prefix": "table",
        "body": [
            "${1:Table}~\\ref{${2:tab:}}$0"
        ],
        "desciption": "Table"
    },
    "Tabular": {
        "prefix": "tab",
        "body": [
            "\\\\begin{${1:t}${1/(t)$|(a)$|(.*)/(?1:abular)(?2:rray)/}}{${2:c}}",
            "$0${2/((?<=[clr])([ |]*(c|l|r)))|./(?1: & )/g}",
            "\\\\end{${1:t}${1/(t)$|(a)$|(.*)/(?1:abular)(?2:rray)/}}"
        ],
        "desciption": "Tabular"
    },
    "\\begin{}…\\end{}": {
        "prefix": "begin",
        "body": [
            "\\\\begin{${1:env}}",
            "\t${1/(enumerate|itemize|list)|(description)|.*/(?1:\\item )(?2:\\item)/}$0",
            "\\\\end{${1:env}}"
        ],
        "desciption": "Begin - End"
    }
}