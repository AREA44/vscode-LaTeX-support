{
    "comments": {
      	"lineComment": "%"
    },
    "brackets": [
		["{", "}"],
		["[", "]"],
		["(", ")"]
	],
	"autoClosingPairs": [
		["{", "}"],
		["[", "]"],
		["(", ")"],
		["$", "$"],
		["`", "'"]
	],
	"surroundingPairs": [
		["{", "}"],
		["[", "]"],
		["(", ")"],
		["\"", "\""],
		["'", "'"],
		["`", "'"],
		["$", "$"]
	],
	"indentationRules": {
		"increaseIndentPattern": "^\\s*\\\\begin\\{.*\\}",
		"decreaseIndentPattern": "^\\s*\\\\end(\\{.*\\})?$"
	}
}