{
    "comments": {
      	"lineComment": "%"
    },
    "brackets": [
		["{", "}"],
		["[", "]"],
		["(", ")"]
	],
	"autoClosingPairs": [
		["{", "}"],
		["[", "]"],
		["(", ")"],
		["$", "$"],
		["`", "'"]
	],
	"surroundingPairs": [
		["{", "}"],
		["[", "]"],
		["(", ")"],
		["\"", "\""],
		["'", "'"],
		["`", "'"],
		["$", "$"]
	],
	"folding": {
		"offSide": "true",
		"markers": {
			"start": "^\\s*%#Region\\b",
			"end": "^\\s*%#Endregion\\b"
		}
	}
}